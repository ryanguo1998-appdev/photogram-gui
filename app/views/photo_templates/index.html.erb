<h1>List of photos</h1>
Add buttons here
<hr>
<table border=1>
  <tbody>
    <tr>
      <th>Image</th>
      <th>Caption</th>
      <th>Owner</th>
      <th>Posted</th>
      <th></th>
  </tbody>
  <% all_photos = Photo.all.order(:created_at => :desc) %>

  <% all_photos.each do |a_photo| %>
    <tr>
      <td><img src="<%=a_photo.image%>"></td>
      <td><%=a_photo.caption%></td>
      <td><%=a_photo.poster.username%></td>
      <td><%=a_photo.created_at%></td>
      <td><a href="/photos/<%=a_photo.id%>">Show details</a></td>
    </tr>
  <% end %>
</table>